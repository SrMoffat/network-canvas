{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "error",
    "no-extra-parens": "error",
    "no-undef": "error",
    "no-unused-vars": "error",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "indent": [
      "error",
      2
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-trailing-spaces": "error"
  }
}